const editorTooltip = document.getElementsByClassName("editor-tooltip")[0];
editorTooltip.style.display = showTooltip ? "block" : "none";
if(showTooltip) {
  const createVideos = document.getElementsByClassName("create")[0];
  const close = document.getElementsByClassName("close")[0];
  const projLink = document.getElementsByClassName("projLink")[0];
  let waitForGlobal = (key, callback) => {
    if (window[key]) {
      callback();
    } else {
      setTimeout(() => {
        waitForGlobal(key, callback);
      }, 100);
    }
  };

  let track = (interaction) => {
    if (window.combinedGtmTracking) {
      window.combinedGtmTracking.gtmPushTracking('notificationTooltipInteraction', {
        'tooltip_interaction': interaction,
        'tooltip_selection': 'nav_tooltip_drafts_reminder',
        'ui_element_location': 'nav_header_tooltip_video_editor'
      });  
    }
  }
  waitForGlobal("combinedGtmTracking", () => { track('Viewed'); });
  // Needed as the tooltip and the create menu overlap with each other. 
  createVideos.addEventListener("mouseenter", event => editorTooltip.style.pointerEvents = "none");
  createVideos.addEventListener("mouseleave", event => editorTooltip.style.pointerEvents = "all");
  let closeTooltip = () => {
    localStorage.setItem('show_editor_draft_reminder', false);
    editorTooltip.style.display = "none";
  }
  if (close)
    close.addEventListener('click', event => {
      closeTooltip();
      track('Close');
    });
  if (projLink)
    projLink.addEventListener('click', event => {
      event.preventDefault();
      closeTooltip();
      track('Click'); 
      location.href = projLink.getAttribute('href');
    });

  // Spanish, German/de, etc. languages occupy occupy 3 and 4 lines respectively and the triangle at the top of the tooltip needs below styling adjustments 
  // Spanish 3 lines
  if (editorTooltip.offsetHeight >= 64) editorTooltip.style.setProperty("--top", "-8%");
  // German 4 lines
  if (editorTooltip.offsetHeight >= 80) editorTooltip.style.setProperty("--top", "-6%");
}</script></li><li class="icon-only boards-icon boards-icon-new" id="open_board"><a href="javascript:void(0);"><span alt="Boards" class="icon-new"></span><span class="action-label new">Boards </span><span class="carrot-container"><span class="carrot"></span></span></a><div class="boards-menu-container"></div></li><li class="thin-header right-off-canvas-toggle-menu" data-istock-user-id=""><figure class="notification-dot" ng-style="{&#39;opacity&#39;: displayRedDot ? &#39;1&#39; : &#39;0&#39;}"></figure><figure class="account"><img class="rest-state" src="/asset-detail/assets/static/istock_global-4918a622f02f75589638.svg#account-icon-mono" viewBox="0 0 100 100" /><img class="hover-state" src="/asset-detail/assets/static/istock_global-4918a622f02f75589638.svg#account-icon" viewBox="0 0 100 100" /></figure><span class="display-name">Account</span><span class="carrot-container"><span class="carrot"></span></span></li><li class="wide-header"><a class="account" data-nav="nav=nav_SignIn" href="/sign-in?returnurl=%2Fphoto%2Frestaurant-gourmet-food-on-dark-background-gm902558446-248957416" login-link="" rel="nofollow">Sign in</a></li><li class="wide-header"><a class="account" data-nav="nav=nav_Join" href="/join" rel="nofollow">Join</a></li></ul></div></nav><div class="dd-user-panel-menu right"><div class="close-panel"><figure class="general-icon-slider-close"></figure></div><ul class="not-remembered"><li><a data-nav="nav=nav_SignIn" href="/sign-in?returnurl=%2Fphoto%2Frestaurant-gourmet-food-on-dark-background-gm902558446-248957416" login-link="">Sign in</a></li><li><a data-nav="nav=nav_Join" href="/join">Join</a></li><li class="mobile"><a data-nav="nav=nav_Account_video_templates" data-ui-location="header_side_panel_right" href="/video-editor/templates">Video templates</a></li><li class="mobile"><a data-nav="nav=nav_Pricing" href="/plans-and-pricing">Plans and pricing</a></li><li class="mobile"><a data-nav="nav=nav_YourBoards" href="/collaboration/boards/default">Your Boards